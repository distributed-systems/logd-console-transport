{
  "name": "logd-console-transport",
  "version": "2.0.0",
  "description": "console transport for logd. prints messages to the console",
  "main": "dist/ConsoleTransport.js",
  "types": "dist/ConsoleTransport.d.ts",
  "scripts": {
    "build": "node ./node_modules/.bin/tsc",
    "start": "node ./node_modules/.bin/tsc && node dist/index.js --l --dev",
    "clean": "rm -r build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distributed-systems/logd-console-transport.git"
  },
  "keywords": [
    "logd",
    "logd-transport",
    "logd-console",
    "console"
  ],
  "author": "Lina van der Weg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/distributed-systems/logd-console-transport/issues"
  },
  "homepage": "https://github.com/distributed-systems/logd-console-transport#readme",
  "devDependencies": {
    "@tsconfig/node18": "^18.2.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "logd": "^4.0.1",
    "logd-console-output": "^2.0.0"
  },
  "type": "module"
}
